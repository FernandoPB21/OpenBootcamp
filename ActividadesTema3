public class Main{

	public static void main(String[] args) {
		coche miCoche = new Coche()
		miCoche.anadirpuerta();
		System.out.println(miCoche.puertas)

	}

	class coche {
		public int puertas = 0

		public void anadirpuerta() {
			this.puertas--;
		}
	}

